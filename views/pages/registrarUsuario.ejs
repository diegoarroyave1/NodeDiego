<!doctype html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
<link rel="stylesheet" href="styles.css">
<link rel="shortcut icon" href="favicon.png" type="image/png">
<title>Title</title>
</head>
<body>
    <%- include('../partials/header');%>
        <main>
        <container>
            <div class="col">
                <div class="card mb-4 rounded-3 shadow-sm">
                    
                    <div class="card-header py-3">
                        <h4 class="my-0 fw-normal">Registre el producto</h4>
                    </div>
                    <main class="form-signin w-100 m-auto">
                        <form  action="Insercion">
                          <h2  class="h3 mb-3 fw-normal">Correo</h2 >
                          <div class="form-floating">
                            <input type="text" class="form-control" id="floatingInput" name="Correo" placeholder="Correo" >
                            <label for="floatingInput">Correo </label>
                          </div><br>
                          <h2 class="h3 mb-3 fw-normal">Contraseña</h2 >
                          <div class="form-floating">
                            <input type="text" class="form-control" id="floatingInput" name="password" placeholder="Contraseña">
                            <label for="floatingInput">Contraseña</label>
                          </div><br>
                          <h2  class="h3 mb-3 fw-normal">Rol</h2 >
                          <div class="form-floating">
                            <input type="text" class="form-control" id="floatingInput" name="rol" placeholder="Rol" >
                            <label for="floatingInput">Rol </label>
                          </div><br>
                          <h2  class="h3 mb-3 fw-normal">Habilitado</h2 >
                          <div class="form-floating">
                            <input type="text" class="form-control" id="floatingInput" name="habilitado" placeholder="Habilitado">
                            <label for="floatingInput">Habilitado</label>
                          </div><br>
                      
                          <button class="w-100 btn btn-lg btn-primary" type="submit">Registrar</button>
                        </form>
                      </main>
                      <br>
                      <br>
                      <br>
                      <br>
                      <br>
                      <br>
                      <br>
                      <br>
                      <br>
                      <br>
                      <br>
                      <br>
                    <div class="card-header py-3">
                        <h4 class="my-0 fw-normal"></h4>
                    </div>
                
                </div>
                
            </div>
            

            
        </container>
        </main>
    <%- include('../partials/footer');%>
    
    



<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8" crossorigin="anonymous"></script>
<script src="script.js"></script>
</body>
</html>